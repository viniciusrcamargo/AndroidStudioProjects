<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.edu.unirp.android.findpetwalker">

    <application
        android:allowBackup="true"
        android:icon="@drawable/findpetwalkericone"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PrincipalActivity" />
        <activity
            android:name=".splashscreen.SplashScreen"
            android:theme="@style/AppTelaCheia">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".InserirActivity" />
        <activity android:name=".EditarActivity" />
        <activity android:name=".ListarUsuarios"></activity>
    </application>

</manifest>